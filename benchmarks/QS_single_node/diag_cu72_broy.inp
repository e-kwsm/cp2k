&GLOBAL
  PROJECT diag_cu72_broy
  ! Do not compute forces to avoid performance cliff of grid_integrate_task_list on GPU.
  RUN_TYPE ENERGY
  &PRINT_ELPA ON
  &END PRINT_ELPA
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  STRESS_TENSOR  ANALYTICAL
  &DFT
    BASIS_SET_FILE_NAME BASIS_MOLOPT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    &MGRID
      NGRIDS 5
      CUTOFF 400
      REL_CUTOFF 40
    &END MGRID
    &QS
      METHOD GPW
      EXTRAPOLATION  USE_PREV_WF
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-6
      ADDED_MOS 100
      &SMEAR  ON
        METHOD FERMI_DIRAC
        ELECTRONIC_TEMPERATURE [K] 300
      &END SMEAR
      &DIAGONALIZATION
         ALGORITHM STANDARD
         EPS_ADAPT 0.01
      &END DIAGONALIZATION
      &MIXING
         METHOD BROYDEN_MIXING
         ALPHA   0.1
         BETA    1.5
         NBROYDEN  8
      &END
      &PRINT
        &RESTART OFF
        &END
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
      &VDW_POTENTIAL
       POTENTIAL_TYPE NON_LOCAL
        &NON_LOCAL
          CUTOFF 300
          TYPE RVV10
          KERNEL_FILE_NAME rVV10_kernel_table.dat
        &END NON_LOCAL
      &END VDW_POTENTIAL
    &END XC
  &END DFT
