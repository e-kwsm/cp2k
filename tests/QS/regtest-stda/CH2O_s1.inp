&GLOBAL
  PROJECT CH2O
  RUN_TYPE ENERGY_FORCE
  PRINT_LEVEL LOW
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &PROPERTIES
    &TDDFPT
       KERNEL sTDA
       &sTDA 
          FRACTION 0.0
          DO_EXCHANGE F
       &END sTDA
       NSTATES      5
       MAX_ITER    50
       CONVERGENCE [eV] 1.0e-8
    &END TDDFPT
  &END PROPERTIES
  &PRINT
     &FORCES
     &END
  &END
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 280
      REL_CUTOFF 60
    &END MGRID
    &QS
    &END QS
    &EXCITED_STATES T
       STATE 1
    &END EXCITED_STATES
    &SCF
      MAX_SCF 10
      EPS_SCF 1.E-7
      SCF_GUESS ATOMIC 
      &OT
         MINIMIZER DIIS
         PRECONDITIONER FULL_ALL
      &END OT
      &OUTER_SCF
         MAX_SCF 5
         EPS_SCF 1.E-7
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
      PERIODIC NONE
    &END CELL
    &TOPOLOGY
     &CENTER_COORDINATES
     &END
    &END TOPOLOGY
    &COORD
  O         0.094933   -0.000368    0.895642
  C        -0.031077   -0.000121   -0.307326
  H        -0.090437    0.947608   -0.895642
  H        -0.094933   -0.947608   -0.895562
    &END COORD
    &KIND O
      BASIS_SET aug-TZV2P-GTH
      POTENTIAL GTH-PBE-q6
    &END KIND
    &KIND C
      BASIS_SET aug-TZV2P-GTH
      POTENTIAL GTH-PBE-q4
    &END KIND
    &KIND H
      BASIS_SET aug-TZV2P-GTH
      POTENTIAL GTH-PBE-q1
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
